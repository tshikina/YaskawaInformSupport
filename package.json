{
    "name": "inform-job-support",
    "displayName": "YASKAWA Inform Job Support",
    "description": "Support INFORM JOB JBI for YASKAWA Motoman",
    "publisher": "tshikina",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/tshikina/YaskawaInformSupport.git"
    },
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "inform",
            "aliases": ["inform", "INFORM JBI for YASKAWA Motoman"],
            "extensions": [".JBI"],
            "configuration": "./language-configuration.json"
        },{
            "id": "cioprglst",
            "aliases": ["CIOPRG.LST", "CIOPRG.LST for YASKAWA Motoman"],
            "filenames": ["CIOPRG.LST"],
            "configuration": "./language-configuration.json"
        },{
            "id": "varnamedat",
            "aliases": ["VARNAME.DAT", "VARNAME.DAT for YASKAWA Motoman"],
            "filenames": ["VARNAME.DAT"],
            "configuration": "./language-configuration.json"
        },{
            "id": "vardat",
            "aliases": ["VAR.DAT", "VAR.DAT for YASKAWA Motoman"],
            "filenames": ["VAR.DAT"],
            "configuration": "./language-configuration.json"
        },{
            "id": "ionamedat",
            "aliases": ["IONAME.DAT", "IONAME.DAT for YASKAWA Motoman"],
            "filenames": ["IONAME.DAT"],
            "configuration": "./language-configuration.json"
        },{
            "id": "iomnamedat",
            "aliases": ["IOMNAME.DAT", "IOMNAME.DAT for YASKAWA Motoman"],
            "filenames": ["IOMNAME.DAT"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "inform",
            "scopeName": "source.inform",
            "path": "./syntaxes/inform.tmLanguage.json"
        },{
            "language": "cioprglst",
            "scopeName": "source.cioprglst",
            "path": "./syntaxes/cioprglst.tmLanguage.json"
        },{
            "language": "varnamedat",
            "scopeName": "source.varnamedat",
            "path": "./syntaxes/varnamedat.tmLanguage.json"
        },{
            "language": "vardat",
            "scopeName": "source.vardat",
            "path": "./syntaxes/vardat.tmLanguage.json"
        },{
            "language": "ionamedat",
            "scopeName": "source.ionamedat",
            "path": "./syntaxes/ionamedat.tmLanguage.json"
        },{
            "language": "iomnamedat",
            "scopeName": "source.iomnamedat",
            "path": "./syntaxes/iomnamedat.tmLanguage.json"
        }]
    }
}